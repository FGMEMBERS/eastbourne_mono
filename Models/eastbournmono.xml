<PropertyList>

 <path>eastbournemono.ac</path>

<effect>
     <inherits-from>Effects/model-transparent</inherits-from>
      <object-name>propdisc</object-name>
      <object-name>propblur</object-name>
    </effect>

  <model>
    <path>Aircraft/eastbourne_mono/Models/Panel/panel.xml</path>
  </model>

  <!-- Pilot -->
  <model>
    <path>Aircraft/eastbourne_mono/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> 0.884 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  -0.25 </z-m>
    </offsets>
  </model>
 
 <!-- Control surfaces -->
 <!--<property>surface-positions/rudder-pos-norm</property>-->
     <animation>
      <type>rotate</type>
      <object-name>Rudder</object-name>
      <object-name>leftruddcable</object-name>
      <object-name>rightruddcable</object-name>
      <property>controls/flight/rudder</property>
      <interpolation>
         <entry>      <ind>1.0</ind>   <dep>29</dep>   </entry>
         <entry>      <ind>-1.0</ind>   <dep>-29</dep>   </entry>
      </interpolation>
      <center>
         <x-m>3.94</x-m>
         <y-m>0.00</y-m>
         <z-m>0.00</z-m>
      </center>
      <axis>
         <x>0</x>
         <y>0</y>
         <z>1</z>
      </axis>
        </animation>

        <animation>
      <type>rotate</type>
      <object-name>leftruddcable</object-name>
      <property>controls/flight/rudder</property>
      <interpolation>
         <entry>      <ind>1.0</ind>   <dep>-29</dep>   </entry>
         <entry>      <ind>-1.0</ind>   <dep>29</dep>   </entry>
      </interpolation>
      <center>
         <x-m>3.94</x-m>
         <y-m>-0.123</y-m>
         <z-m>0.00</z-m>
      </center>
      <axis>
         <x>0</x>
         <y>0</y>
         <z>1</z>
     </axis>
    </animation>

    <animation>
     <type>rotate</type>
     <object-name>rightruddcable</object-name>
     <property>controls/flight/rudder</property>
     <interpolation>
      <entry>      <ind>1.0</ind>   <dep>-29</dep>   </entry>
      <entry>      <ind>-1.0</ind>   <dep>29</dep>   </entry>
     </interpolation>
     <center>
      <x-m>3.94</x-m>
      <y-m>0.123</y-m>
      <z-m>0.00</z-m>
     </center>
     <axis>
      <x>0</x>
      <y>0</y>
      <z>1</z>
   </axis>
 </animation>

<animation>
      <type>rotate</type>
      <object-name>Elevator</object-name>
      <object-name>upelevcable</object-name>
      <object-name>lowelevcable</object-name>
      <property>controls/flight/elevator</property>
      <interpolation>
         <entry>      <ind>1.0</ind>   <dep>20</dep>   </entry>
         <entry>      <ind>-1.0</ind>   <dep>-20</dep>   </entry>
      </interpolation>
      <center>
         <x-m>3.954</x-m>
         <y-m>0.00</y-m>
         <z-m>0.082</z-m>
      </center>
      <axis>
         <x>0</x>
         <y>1</y>
         <z>0</z>
      </axis>
        </animation>

        <animation>
      <type>rotate</type>
      <object-name>upelevcable</object-name>
      <property>controls/flight/elevator</property>
      <interpolation>
         <entry>      <ind>1.0</ind>   <dep>-20</dep>   </entry>
         <entry>      <ind>-1.0</ind>   <dep>20</dep>   </entry>
      </interpolation>
      <center>
         <x-m>3.959</x-m>
         <y-m>0.00</y-m>
         <z-m>0.202</z-m>
      </center>
      <axis>
         <x>0</x>
         <y>1</y>
         <z>0</z>
     </axis>
    </animation>

    <animation>
     <type>rotate</type>
     <object-name>lowelevcable</object-name>
     <property>controls/flight/elevator</property>
     <interpolation>
      <entry>      <ind>1.0</ind>   <dep>-20</dep>   </entry>
      <entry>      <ind>-1.0</ind>   <dep>20</dep>   </entry>
     </interpolation>
     <center>
      <x-m>3.959</x-m>
      <y-m>0.00</y-m>
      <z-m>-0.037</z-m>
     </center>
     <axis>
      <x>0</x>
      <y>1</y>
      <z>0</z>
   </axis>
 </animation>



 <animation>
  <type>rotate</type>
  <object-name>Leftaileron</object-name>
  <property>surface-positions/left-aileron-pos-norm</property>
  <factor>20</factor>
  <center>
   <x-m>0.683</x-m>
   <z-m>-0.023</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>Rightaileron</object-name>
  <property>surface-positions/right-aileron-pos-norm</property>
  <factor>20</factor>
  <center>
   <x-m>0.683</x-m>
   <z-m>-0.023</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- cockpit animations -->

<!-- wheel yoke assembly -->

 <animation>
  <type>rotate</type>
  <object-name>yokemast</object-name>
  <object-name>yokewheel</object-name>
  <property>controls/flight/elevator</property>
  <factor>15</factor>
  <center>
    <x-m>0.333</x-m>
   <y-m> 0.00</y-m>
   <z-m> -0.3697</z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>yokewheel</object-name>
  <property>controls/flight/aileron</property>
  <factor>-84</factor>
  <center> 
   <x-m>0.333</x-m>
   <y-m>0.00</y-m>
   <z-m>0.037</z-m>
  </center>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

<!-- rudderbar -->

 <animation>
  <type>rotate</type>
  <object-name>rudderbar</object-name>
  <property>controls/flight/rudder</property>
  <factor>24</factor>
  <center>
   <x-m>-0.0136</x-m>
   <y-m>0.00</y-m>
   <z-m>-0.362</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0.0</y>
   <z>1.0</z>
  </axis>
 </animation>


<!-- throttle -->

<animation>
    <type>rotate</type>
    <object-name>throttle-lever</object-name>
    <property>controls/engines/engine/throttle</property>
    <factor> -49 </factor>
    <center>
   <x-m>0.281</x-m>
   <y-m>-0.22</y-m>
   <z-m>-0.0449</z-m>
  </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z>  0 </z>
    </axis>
  </animation>

<!-- magneto -->

<animation>
  <type>pick</type>
  <object-name>on-mags</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine/magnetos</property>
    <step>1.0</step>
    <min>0</min>
    <max>3</max>
    <wrap>0</wrap>
    </binding>
  </action>
</animation>

<animation>
  <type>pick</type>
  <object-name>off-mags</object-name>
  <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <binding>
      <command>property-adjust</command>
      <property>/controls/engines/engine/magnetos</property>
    <step>-1.0</step>
    <min>0</min>
    <max>3</max>
    <wrap>0</wrap>
    </binding>
  </action>
</animation>

  <animation>
  <type>rotate</type>
  <object-name>switch</object-name>
  <property>/controls/engines/engine/magnetos</property>
  <interpolation>
   <entry><ind> 0</ind><dep> 0</dep></entry>
   <entry><ind> 1</ind><dep> -30</dep></entry>
   <entry><ind> 2</ind><dep> -60</dep></entry>
   <entry><ind> 3</ind><dep> -90.0</dep></entry>
  </interpolation>
    <center>
   <x-m>0.281</x-m>
   <y-m>-0.160</y-m>
   <z-m> 0.203</z-m>
  </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z>  0 </z>
    </axis>
  </animation>

<animation>
   <type>pick</type>
   <object-name>Propeller</object-name>
   <visible>true</visible>
   <action>
     <name>start</name>
     <button>0</button>
     <binding>
       <command>property-assign</command>
       <property>/controls/engines/engine/starter</property>
       <value>true</value>
     </binding>
     <mod-up>
       <binding>
         <command>property-assign</command>
         <property>/controls/engines/engine/starter</property>
         <value>0</value>
       </binding>
     </mod-up>
   </action>
  </animation>

  <!-- propeller selection with rpm spin -->

  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
    <condition>
      <greater-than>
        <property>engines/engine/rpm</property>
        <value>900</value>
      </greater-than>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>propblur</object-name>
    <condition>
    <and>
      <greater-than>
        <property>engines/engine/rpm</property>
        <value>500.000</value>
      </greater-than>
      <less-than>
        <property>engines/engine/rpm</property>
        <value>900.001</value>
      </less-than>
    </and>
    </condition>
  </animation>

  <animation>
    <type>select</type>
    <object-name>Propeller</object-name>
    <condition>
      <less-than>
        <property>engines/engine/rpm</property>
	<value>500</value>
      </less-than>
    </condition>
  </animation>

  <animation>
    <name>PropComplete</name>
    <object-name>Propeller</object-name>
    <object-name>prop-shaft</object-name>
    <object-name>propdisc</object-name>
    <object-name>propblur</object-name>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>PropComplete</object-name>
    <property>engines/engine/rpm</property>
    <factor> 1 </factor>
    <center>
      <x-m> -0.490 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  0.00 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Gear Compression -->
  <animation>
    <name>Maingear</name>
    <object-name>Leftwheel</object-name>
    <object-name>Rightwheel</object-name>
    <object-name>LG-axle</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Maingear</object-name>
    <property>gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.5 </ind><dep>  -1.15 </dep></entry>
      <entry><ind> 1.0 </ind><dep>   -2.4 </dep></entry>
    </interpolation>
    <center>
      <x-m>  -0.3078 </x-m>
      <y-m>  0.602 </y-m>
      <z-m> -0.962 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>


  <animation>
    <type>rotate</type>
    <object-name>Maingear</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.5 </ind><dep>  -1.15 </dep></entry>
      <entry><ind> 1.0 </ind><dep>   -2.4 </dep></entry>
    </interpolation>
    <center>
      <x-m>  -0.3078 </x-m>
      <y-m>  -0.602 </y-m>
      <z-m> -0.962 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <name>tailgear-assem</name>
    <object-name>tailskid</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LG-axle-skid</object-name>
    <property>/gear/gear[2]/compression-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep> 0.0 </dep></entry>
      <entry><ind> 0.5 </ind><dep> 3.75 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 6.5 </dep></entry>
    </interpolation>
    <center>
      <x-m>  -0.512 </x-m>
      <y-m>  0.00 </y-m>
      <z-m> 2.521 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> -1 </y>
      <z> 0 </z>
    </axis>
  </animation>

 <animation>
  <type>spin</type>
  <object-name>Rightwheel</object-name>
  <condition>
  <property>/gear/gear[1]/wow</property>
  </condition>
  <property>velocities/groundspeed-kt</property>
  <factor>13</factor> <!-- Wheel circumference 1.541 m -->
  <center>
   <x-m>-0.307</x-m>
   <y-m>0.258</y-m>
   <z-m>-0.984</z-m>
  </center>
  <axis>
   <x> 0.0</x>
   <y>-1.0</y>
   <z> 0.0</z>
  </axis>
  </animation>

 <animation>
  <type>spin</type>
  <object-name>Leftwheel</object-name>
  <condition>
  <property>/gear/gear[0]/wow</property>
  </condition>
  <property>velocities/groundspeed-kt</property>
  <factor>13</factor> <!-- Wheel circumference 1.541 m -->
  <center>
   <x-m>-0.307</x-m>
   <y-m>-0.258</y-m>
   <z-m>-0.984</z-m>
  </center>
  <axis>
   <x> 0.0</x>
   <y>-1.0</y>
   <z> 0.0</z>
  </axis>
  </animation>

</PropertyList>
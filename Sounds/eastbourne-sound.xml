<?xml version="1.0" encoding="UTF-8"?>
 
<PropertyList>

  <fx>
 
    <crank>
      <name>crank</name>
      <path>Aircraft/eastbourne_mono/Sounds/anzani-start.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine/cranking</property>
        <not>
          <property>/engines/engine/running</property>
        </not>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>0.5</min>
        <max>1.0</max>
      </volume>
      <volume> 
        <factor>1.2</factor>
        <max>1.0</max>
      </volume>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine>
      <name>engine</name>
      <path>Aircraft/eastbourne_mono/Sounds/EngineRun.wav</path>
      <mode>looped</mode>
      <condition>
        <property>/engines/engine/running</property>
        <greater-than>
          <property>/engines/engine/rpm</property>
          <value>150</value>
        </greater-than>
      </condition>
      <volume>
        <property>/engines/engine/mp-osi</property>
        <factor>0.00375</factor>
        <min>0.5</min>
        <max>1.85</max>
        <offset>0.05</offset>
      </volume>
      <pitch>
        <property>/engines/engine/rpm</property>
        <factor>0.0007</factor>
        <min>0.3</min>
        <max>2.8</max>
        <offset>0.05</offset>
      </pitch>
      <reference-dist>180</reference-dist>
      <max-dist>1500</max-dist>
    </engine>

  <propeller>
      <name>propeller</name>
      <mode>looped</mode>
      <path>Aircraft/eastbourne_mono/Sounds/cessna120-prop.wav</path>
      <property>/engines/engine/running</property>
      <volume>
        <property>/engines/engine/mp-osi</property>
        <factor>0.0075</factor>
        <min>0.5</min>
        <max>1.85</max>
        <offset>0.05</offset>
      </volume>
      <pitch>
        <property>/engines/engine/mp-osi</property>
        <factor>0.0009</factor>
        <min>0.1</min>
        <max>2.0</max>
        <offset>0.9</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </propeller>

    <squeal>
      <name>squeal</name>
      <path>Sounds/squeal.wav</path>
      <condition>
        <or>
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
        </or>
      </condition>
      <volume>
        <property>/velocities/speed-down-fps</property>
        <factor>0.05</factor>
      </volume>
      <volume>
        <property>/velocities/airspeed-kt</property>
        <factor>0.01</factor>
      </volume>
      <volume>
        <property>dt_stop</property>
        <factor>0.05</factor>
        <max>1.0</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property>
        <factor>0.0025</factor>
        <offset>1.2</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </squeal>

<rumble>
   <name>rumble</name>
   <mode>looped</mode>
   <path>Sounds/rumble.wav</path>
   <condition>
    <or>
     <property>/gear/gear[0]/wow</property>
     <property>/gear/gear[1]/wow</property>
     <property>/gear/gear[2]/wow</property>
    </or>
   </condition>
   <volume>
    <property>/velocities/airspeed-kt</property>
    <type>log</type>
    <factor>0.5</factor> <!-- 0.25 -->
   </volume>
  </rumble>

    <wind>
      <name>wind</name>
      <mode>looped</mode>
      <path>Aircraft/eastbourne_mono/Sounds/wind.wav</path>
      <property>/velocities/airspeed-kt</property>
      <volume>
        <property>/position/altitude-ft</property>
        <factor>-0.000015</factor>
        <offset>1.0</offset>
        <min>0.1</min>
        <max>1.0</max>
      </volume>
      <volume>
         <property>/velocities/airspeed-kt</property>
         <factor>0.003</factor>
         <min>0.03</min>
         <max>1.2</max>
      </volume>
      <pitch>
        <property>/velocities/airspeed-kt</property> 
        <factor>0.003</factor>
        <offset>1.25</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </wind>

    <stall>
   <name>stall</name>
   <mode>looped</mode>
   <type>avionics</type>
   <path>Aircraft/eastbourne_mono/Sounds/StallBuffet2.wav</path>
   <condition>
    <and>
     <property>sim/alarms/stall-warning</property>
     <greater-than>
       <property>velocities/airspeed-kt</property>
       <value>10</value>
     </greater-than>
     <not>
       <property>gear/gear[0]/wow</property>
       <property>gear/gear[1]/wow</property>
       <property>gear/gear[2]/wow</property>
     </not>
    </and>
   </condition>
   <volume>
    <property>sim/alarms/stall-warning</property>
    <max>1.0</max>
   </volume>
   <reference-dist>10.0</reference-dist>
   <max-dist>20.0</max-dist>
  </stall>

    <!--                  Toggle Switches                  -->

    <click>
      <name>Magnetos-off</name>
      <path>Sounds/click.wav</path>
      <condition>
        <not>
          <property>controls/engines/engine/magnetos</property>
        </not>
      </condition>
      <volume>
        <factor>1.0</factor>
      </volume>
    </click>

    <click>
      <name>Magnetos-R</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine/magnetos</property>
          <value>1</value>
        </equals>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </click>

    <click>
      <name>Magnetos-L</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine/magnetos</property>
          <value>2</value>
        </equals>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </click>

    <click>
      <name>Magnetos-B</name>
      <path>Sounds/click.wav</path>
      <condition>
        <equals>
          <property>controls/engines/engine/magnetos</property>
          <value>3</value>
        </equals>
      </condition>
      <volume>
        <factor>0.8</factor>
      </volume>
      <reference-dist>0.5</reference-dist>
      <max-dist>5.0</max-dist>
    </click>

  </fx>
 
</PropertyList>
